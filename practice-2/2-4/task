#include <stdio.h>
#include <stdlib.h>

int main() {
    // Выделяем память под массив из 10 целых чисел
    int *array = (int *)malloc(10 * sizeof(int));
    if (array == NULL) {
        perror("Failed to allocate memory");
        return EXIT_FAILURE;
    }

    // Инициализируем массив
    for (int i = 0; i < 10; i++) {
        array[i] = i * i; // Заполняем массив квадратами чисел
    }

    // Печатаем значения массива
    for (int i = 0; i < 10; i++) {
        printf("%d ", array[i]);
    }
    printf("\n");

    // Освобождаем выделенную память
    free(array);
    return EXIT_SUCCESS;
}
